<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>GoodsServlet</servlet-name>
        <servlet-class>com.wnn.cookie.GoodsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoodsServlet</servlet-name>
        <url-pattern>/GoodsServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObtainServlet</servlet-name>
        <servlet-class>com.wnn.cookie.ObtainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ObtainServlet</servlet-name>
        <url-pattern>/ObtainServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MySessionServlet</servlet-name>
        <servlet-class>com.wnn.cookie.MySessionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySessionServlet</servlet-name>
        <url-pattern>/MySessionServlet</url-pattern>
    </servlet-mapping>
    
</web-app>